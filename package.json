{
    "name": "operations-service",
    "version": "1.0.0",
    "scripts": {
      "start": "ts-node src/server.ts",
      "build": "tsc",
      "test": "jest",
      "migrate:create": "node-pg-migrate create",
      "migrate:up": "ts-node node_modules/.bin/node-pg-migrate up -m ./migrations -j ts",
      "migrate:down": "ts-node node_modules/.bin/node-pg-migrate down -m ./migrations -j ts",
      "migrate:redo": "ts-node node_modules/.bin/node-pg-migrate redo -m ./migrations -j ts"
    },
    "dependencies": {
      "ws": "^8.16.0",
      "pg": "^8.11.3",
      "node-pg-migrate": "^6.2.2",
      "pino": "^8.17.2",
      "dotenv": "^16.3.1"
    },
    "devDependencies": {
      "@types/ws": "^8.5.10",
      "@types/node": "^20.11.0",
      "@types/jest": "^29.5.11",
      "@types/pg": "^8.10.9",
      "typescript": "^5.3.3",
      "ts-node": "^10.9.2",
      "jest": "^29.7.0"
    }
  }